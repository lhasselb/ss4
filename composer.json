{
    "name": "silverstripe/installer",
    "type": "silverstripe-recipe",
    "description": "The SilverStripe Framework Installer",
    "repositories": [
        {
            "url": "https://github.com/lhasselb/clubmaster.git",
            "type": "git"
        },
        {
            "url": "https://github.com/lhasselb/silverstripe-dataobject-links.git",
            "type": "git"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/recipe-cms": "^4.3.0@stable",
        "silverstripe-themes/simple": "~3.2.0",
        "lhasselb/clubmaster": "dev-master",
        "gorriecoe/silverstripe-linkfield": "dev-master",
        "undefinedoffset/sortablegridfield": "^2.0",
        "silverstripe/tagfield": "^2.2",
        "jinjie/ss4-duplicate-dataobject": "^1.1",
        "stevie-mayhew/hasoneedit": "^2.0",
        "axllent/silverstripe-scaled-uploads": "^2.2",
        "jonom/focuspoint": "^3.0",
        "flxlabs/silverstripe-dataobject-links": "dev-master",
        "drmartingonzo/ss-tinymce-charcount": "^1.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "gdmedia/ss-auto-git-ignore": "^1.0",
        "dynamic/silverstripe-classname-update-tasks": "^1.0@alpha"
    },
    "extra": {
        "expose": [
            "app/client/dist"
        ],
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
